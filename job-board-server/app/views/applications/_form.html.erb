<%= form_for @application do |f| %>

<%= f.label :name %>
<%= f.text_field :name%>
<%= f.label :resume %>
<%= f.text_field :resume %>
<%= f.label :cover_letter %>
<%= f.text_field :cover_letter %>
<%= f.label :job_id %>
<%= f.select :job_id, Job.pluck(:title,:id), :include_blank => true %>
<%= f.label :seeker_id %>
<%= f.select :seeker_id, Seeker.pluck(:name,:id), :include_blank => true %>
<%= f.submit %>
<% end %>
