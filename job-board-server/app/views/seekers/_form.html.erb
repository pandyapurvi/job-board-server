
<%= form_for @seeker do |f| %>

<%= f.label :name %>
<%= f.text_field :name %>
<%= f.label :emailid %>
<%= f.text_field :emailid %>
<%= f.label :pnone %>
<%= f.number_field :pnone %>
<%= f.label :social_link %>
<%= f.text_field :social_link %>
<%= f.label :resume %>
<%= f.text_field :resume %>
<%= f.label :cover_letter %>
<%= f.text_field :cover_letter %>
<%= f.label :experience_year %>
<%= f.number_field :experience_year ,:min=>'0' %>
<%= f.label :notice %>
<%= f.text_field :notice %>
<%= f.label :current_title %>
<%= f.text_field :current_title %>

<%= f.submit %>

<% end %>
