
<%= form_for @job do |f| %>

<%= f.label :title %>
<%= f.text_field :title%>
<%= f.label :description %>
<%= f.text_field :description %>
<%= f.label :level %>
<%= f.text_field :level %>
<%= f.label :company_type %>
<%= f.text_field :company_type %>
<%= f.label :salary %>
<%= f.number_field :salary %>
<%= f.label :duration %>
<%= f.text_field :duration %>
<%= f.label :city %>
<%= f.text_field :city %>

<%= f.submit %>
<% end %>
